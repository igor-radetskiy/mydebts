<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="mydebts.android.app">

    <application
        android:name=".MyDebtsApplication"
        android:allowBackup="true"
        android:fullBackupContent="false"
        android:label="@string/app_name"
        android:supportsRtl="false"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning,MissingApplicationIcon">
        <activity android:name=".ui.events.EventsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".feature.person.PersonActivity" />
        <activity
            android:name=".feature.people.PeopleActivity"
            android:label="@string/people_title" />
        <activity android:name=".feature.event.EventActivity" />
    </application>

</manifest>